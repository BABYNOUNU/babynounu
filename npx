mkdir ./src/app/jobs &&
npx nest generate module app/job --no-spec &&
npx nest generate controller app/job --no-spec &&
npx nest generate service app/job --no-spec &&
npx nest generate provider app/job --no-spec &&
mkdir ./src/app/job/models &&
mkdir ./src/app/job/dtos &&
touch ./src/app/job/dtos/create-job.dto.ts &&
touch ./src/app/job/dtos/update-job.dto.ts &&
touch ./src/app/job/models/job.model.ts

git init
git add ./
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Dolubux/njia-backend-app.git
git push -u origin main
