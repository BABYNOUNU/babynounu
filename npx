mkdir ./src/app/upload &&
npx nest generate module app/upload --no-spec &&
npx nest generate controller app/upload --no-spec &&
npx nest generate service app/upload --no-spec &&
npx nest generate provider app/upload --no-spec &&
mkdir ./src/app/upload/models &&
mkdir ./src/app/upload/dto &&
touch ./src/app/upload/models/upload.model.ts

git init
git add ./
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Dolubux/njia-backend-app.git
git push -u origin main
