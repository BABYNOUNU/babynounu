mkdir ./src/app/profiles &&
npx nest generate module app/profiles --no-spec &&
npx nest generate controller app/profiles --no-spec &&
npx nest generate service app/profiles --no-spec &&
npx nest generate provider app/profiles --no-spec &&
mkdir ./src/app/profiles/models &&
mkdir ./src/app/profiles/dtos &&
touch ./src/app/profiles/dtos/create-profile.dto.ts &&
touch ./src/app/profiles/dtos/update-profile.dto.ts &&
touch ./src/app/profiles/models/profile.model.ts

git init
git add ./
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Dolubux/njia-backend-app.git
git push -u origin main
