mkdir ./src/app/nounus &&
npx nest generate module app/nounus --no-spec &&
npx nest generate controller app/nounus --no-spec &&
npx nest generate service app/nounus --no-spec &&
npx nest generate provider app/nounus --no-spec &&
mkdir ./src/app/nounus/models &&
mkdir ./src/app/nounus/dtos &&
touch ./src/app/nounus/dtos/create-nounu.dto.ts &&
touch ./src/app/nounus/dtos/update-nounu.dto.ts &&
touch ./src/app/nounus/models/nounu.model.ts

git init
git add ./
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Dolubux/njia-backend-app.git
git push -u origin main
